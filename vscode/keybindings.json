[
  {
    "key": "ctrl+shift+x",
    "command": "workbench.action.terminal.kill",
    "when": "terminalFocus"
  },
  {
    "key": "ctrl+e",
    "command": "expand-selection-to-scope.expand"
  },
  {
    "key": "ctrl+pagedown",
    "command": "workbench.action.terminal.focusNext",
    "when": "terminalFocus && terminalHasBeenCreated && !terminalEditorFocus || terminalFocus && terminalProcessSupported && !terminalEditorFocus"
  },
  {
    "key": "ctrl+pageup",
    "command": "workbench.action.terminal.focusPrevious",
    "when": "terminalFocus && terminalHasBeenCreated && !terminalEditorFocus || terminalFocus && terminalProcessSupported && !terminalEditorFocus"
  },
  {
    "key": "pageup",
    "command": "workbench.action.terminal.scrollUp",
    "when": "terminalFocusInAny && terminalHasBeenCreated && !terminalAltBufferActive || terminalFocusInAny && terminalProcessSupported && !terminalAltBufferActive"
  },
  {
    "key": "pagedown",
    "command": "workbench.action.terminal.scrollDown",
    "when": "terminalFocusInAny && terminalHasBeenCreated && !terminalAltBufferActive || terminalFocusInAny && terminalProcessSupported && !terminalAltBufferActive"
  },
  {
    "key": "ctrl+shift+o",
    "command": "workbench.action.output.toggleOutput",
    "when": "workbench.panel.output.active"
  },
  {
    "key": "ctrl+t",
    "command": "fzf-haskell.findTag"
  },
  {
    "key": "ctrl+shift+j",
    "command": "fzf-haskell.regenerateTags"
  },
  {
    "key": "ctrl+shift+f",
    "command": "fzf-haskell.findAll"
  },
  {
    "key": "ctrl+tab",
    "command": "fzf-haskell.switchTab"
  },
  {
    "key": "ctrl+k ctrl+o",
    "command": "fzf-haskell.openFolder"
  },
  {
    "key": "ctrl+o",
    "command": "fzf-haskell.openFile"
  },
  {
    "key": "alt+h",
    "command": "fzf-haskell.hoogleSearch"
  },
  {
    "key": "ctrl+k w",
    "command": "workbench.action.closeOtherEditors"
  }
]